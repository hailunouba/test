function f(num) {
    var sum = 0;
    sum += num;
    var func = function(param) {
        sum += param;
        return func;
    }
    return func;
}


thirdï¼š
var arr=[1,2,4]
		var s =3;
		function x(arr,s){
			var arr1 = [];
			for(var i =0;i<arr.length;i++) {
				if(arr[i]>=s) {
					return -1;
				}
				else {
					for(var j=i;j<arr.length;j++) {
						//console.log(arr[j]);
						if(arr[j]>=s) {break;}
						else {
							if(arr[i]+arr[j]==s) {
								
								arr1.push(arr[i],arr[j]);
								return arr1;
							}
						}
					}
				}
			}
		}
		x(arr,s)
